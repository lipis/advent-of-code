{
  "devDependencies": {
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.py": [
      "black",
      "git add"
    ],
    "*.{json, ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "advent-of-code-lipis",
  "scripts": {
    "fix:other": "yarn prettier --write",
    "fix:py": "black .",
    "fix": "yarn fix:py && yarn fix:other",
    "prettier": "prettier \"**/*.{json,ts}\"",
    "test:other": "yarn prettier --list-different",
    "test:py": "black --check .",
    "test": "yarn test:other && yarn test:py"
  },
  "version": "0.0.1"
}
